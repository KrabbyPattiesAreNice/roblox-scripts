local function FKPGJGT_fake_script()
	local script = Instance.new('LocalScript', CloseButton)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Parent.Parent.MainFrame.Visible = false
		script.Parent.Parent.Parent.Parent.Open.Visible = true
	end)
end
coroutine.wrap(FKPGJGT_fake_script)()
local function LRQCZPH_fake_script()
	local script = Instance.new('LocalScript', Dex)

	script.Parent.MouseButton1Click:connect(function()
		loadstring(game:GetObjects("rbxassetid://418957341")[1].Source)()
	end)
end
coroutine.wrap(LRQCZPH_fake_script)()
local function FAUTZ_fake_script()
	local script = Instance.new('LocalScript', MainFrame)

	script.Parent.Draggable = true
end
coroutine.wrap(FAUTZ_fake_script)()
local function KPSX_fake_script()
	local script = Instance.new('LocalScript', Fathom)

	script.Parent.MouseButton1Click:connect(function()
		loadstring(game:HttpGet("https://pastebin.com/raw/UmhaEvTT",true))()
	end)
end
coroutine.wrap(KPSX_fake_script)()
local function GOKJZET_fake_script()
